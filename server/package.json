{
  "name": "evalytics-backend",
  "version": "1.0.0",
  "description": "Backend for Evalytics application",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "db:test": "node test-db.js",
    "db:migrate:questions": "node scripts/migrate-questions.js",
    "db:migrate:evaluations": "node scripts/migrate-evaluations.js",
    "db:migrate:add-user-department": "node scripts/migrate-add-user-department.js",
    "db:migrate:evaluation-answers": "node scripts/migrate-evaluation-answers.js",
    "db:migrate:evaluation-responses-indexes": "node scripts/migrate-evaluation-responses-indexes.js",
    "db:migrate:evaluation-responses": "node scripts/migrate-evaluation-responses.js",
    "db:backfill:students": "node scripts/backfill-students.js",
    "test": "cross-env NODE_ENV=test jest --runInBand",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage --runInBand"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.10.0",
    "uuid": "^11.1.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "cross-env": "^7.0.3"
